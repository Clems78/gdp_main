<launch>
    <group>
        <node pkg="iq_gnc" type="trydrone1" name="drone1_node" output="screen" ns="/drone1">
            <param name="namespace" value="/drone1"/>
            <param name="use_sim_time" value="true" />
        </node>
    </group>

    <group>
        <node pkg="iq_gnc" type="trydrone2" name="drone2_node" output="screen" ns="/drone2">
            <param name="namespace" value="/drone2"/>
            <param name="use_sim_time" value="true" />
        </node>
    </group>
    <!-- Omit other drones or add more nodes as needed -->
</launch>
